{
  "version": "0.1",
  "ignorePaths": [
    "node_modules/**",
    "dits/**",
    "reports/**",
    "package-lock.json",
    "*.svg",
    ".vscode/extensions.json"
  ],
  "language": "en",
  "import": ["@cspell/dict-ru_ru/cspell-ext.json"],
  "overrides": [
    {
      "language": "en,ru",
      "filename": "**/*-ru.json"
    }
  ],
  "words": [
    "automerge",
    "browserslist",
    "cimode",
    "codecov",
    "codeframe",
    "depcruise",
    "graphviz",
    "minmax",
    "mockapi",
    "mqpacker",
    "peaceiris",
    "printf",
    "rankdir",
    "repos",
    "Rodionov",
    "rodmax",
    "shellcheck",
    "stylelint",
    "testcafe"
  ]
}
